{
  "name": "exosuit",
  "version": "0.4.0",
  "authors": [
    "Uplift <git@upliftingcode.com>"
  ],
  "description": "A collection of mixins to be used to augment backbone applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/uplift/ExoSuit"
  },
  "moduleType": [
    "amd",
    "globals",
    "node"
  ],
  "keywords": [
    "exosuit",
    "backbone",
    "mixins",
