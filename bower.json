  ],
  "dependencies": {
    "backbone": "~1.2.0",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "mocha": "~2.0.1",
    "chai": "~3.0.0",
    "jquery": "~2.1.1",
    "sinon": "http://sinonjs.org/releases/sinon-1.9.0.js",
    "chai-jquery": "~2.0.0",
